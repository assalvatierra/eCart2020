@model eCartModels.UserDetail

@{
    ViewBag.Title = "Details";
}

<h2>User Details</h2>


<div class="row panel bordered" style="padding:10px 20px; ">
    <div class="col-lg-12">
        <h2> @Model.Name </h2>

        <p>
            @Html.ActionLink("Edit", "Edit", new { id = Model.Id }) |
            @Html.ActionLink("Back to List", "Index")
        </p>

        <hr />
    </div>
    <div class="col-lg-6">
        <div class="control-label">
            <label class="text-muted"> Address: </label>
            <label> @Model.Address  </label>
        </div>
        <div class="control-label">
            <label class="text-muted"> Email: </label>
            <label> @Model.Email  </label>
        </div>

        <div class="control-label">
            <label class="text-muted"> Mobile: </label>
            <label> @Model.Mobile  </label>
        </div>
        <div class="control-label">
            <label class="text-muted"> Remarks: </label>
            <label> @Model.Remarks  </label>
        </div>
    </div>
    <div class="col-lg-6">
        <div class="control-label">
            <label class="text-muted"> Area: </label>
            <label> @Model.MasterArea.Name  </label>
        </div>
        <div class="control-label">
            <label class="text-muted"> City: </label>
            <label> @Model.MasterCity.Name  </label>
        </div>

        <div class="control-label">
            <label class="text-muted"> Status: </label>
            <label> @Model.UserStatu.Name  </label>
        </div>
    </div>
    </div>